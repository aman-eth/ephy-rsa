{
    "compilerOptions": {
      "lib": ["ES2015", "DOM"],
      "target": "ES6",
      "module": "CommonJS",
      "declaration": true,
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "noImplicitAny": true
    },
    "include": ["src"]
  }
  